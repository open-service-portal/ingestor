#!/usr/bin/env node

/**
 * Ingestor CLI entry point
 * This script uses ts-node to run the xrd-transform CLI
 * (The old ingestor CLI was removed during refactoring)
 */

require('ts-node').register({
  project: require('path').join(__dirname, '..', 'tsconfig.cli.json')
});
require('../src/xrd-transform/cli/xrd-transform-cli');